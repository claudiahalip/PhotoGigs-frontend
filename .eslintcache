 \"FETCHING_USER\":\n                return {...state, loading:true}\n            case \"ADD_USER\":\n                return {...state, users: [...state.users, action.payload]}\n\n        }\n    }\n","/Users/claudia/photogigs-frontend/src/reducers/photographerReducer.js",[],"/Users/claudia/photogigs-frontend/src/containers/PhotographersContainer.js",[],"/Users/claudia/photogigs-frontend/src/containers/ReviewsContainer.js",[],"/Users/claudia/photogigs-frontend/src/containers/UsersContainer.js",["104","105","106"],"import React from 'react'\nimport {Component} from 'react'\nimport conect from 'react-redux'\nimport {Switch, Route, Link} from 'react-router-dom'\nimport Signup from '../components/Signup'\nimport Login from '../components/Login'\n\nclass UsersContainer extends Component {\n    render(){\n        return(\n            <div>\n                \n                    <Route exact path=\"/login\">\n                       <Login/>\n                    </Route>\n                    <Route exact path=\"/signup\">\n                        <Signup/>\n                    </Route>\n                \n            </div>\n        )\n    }\n}\n\nexport default UsersContainer","/Users/claudia/photogigs-frontend/src/components/NavBar.js",[],"/Users/claudia/photogigs-frontend/src/components/Home.js",[],"/Users/claudia/photogigs-frontend/src/components/Reviews.js",[],"/Users/claudia/photogigs-frontend/src/components/Photographers.js",["107"],"\nimport React, {Component} from 'react';\nimport Photographer from './Photographer';\nimport PhotographerAllInfo from './PhotographerAllInfo'\nimport {\n  Route\n} from \"react-router-dom\";\n\nclass Photographers extends Component {\n\n  state = {\n    searchTerm: \"\",\n    currentDisplay: this.props.photographers\n  }\n\n  handleChange =(event)=>{\n    const filteredPhotographers = this.props.photographers.filter(photographer => photographer.name.includes(event.target.value))\n    this.setState({\n      searchTerm: event.target.value,\n      currentDisplay : filteredPhotographers\n    })\n  }\n\n  renderPhotographers = ()=>{\n\n    if (this.state.searchTerm == \"\"){\n    return this.props.photographers\n     .map(photographer => {\n      return <Photographer photographer={photographer} key={photographer.id} />\n      })}\n      else{\n        return this.state.currentDisplay.map(photographer => {\n          return <Photographer photographer={photographer} key={photographer.id} />\n          })\n      }\n  }\n\n  render(){\n      return (\n        <div >\n          <label>Search:</label>\n          <input type=\"text\" onChange={this.handleChange}/>\n          <br></br>\n          {this.renderPhotographers()}\n          <Route exact path=\"/photographers/:id\" component={({match})=>{\n              return <PhotographerAllInfo id = {match.params.id} photographers={this.props.photographers} reviews={this.props.reviews} createReview={this.props.createReview} fetchingReviews={this.props.fetchingReviews}/>\n            }}>\n          </Route>\n          <br></br>\n          <br></br>\n        </div>\n      )\n    }\n\n\n}\n\nexport default Photographers;","/Users/claudia/photogigs-frontend/src/components/PhotographerForm.js",[],"/Users/claudia/photogigs-frontend/src/components/Signup.js",[],"/Users/claudia/photogigs-frontend/src/components/Login.js",["108"],"import React, { Component } from 'react'\nimport axios from 'axios'\nimport App from '../App'\n\nclass Login extends Component {\n    state = {\n        username: \"\",\n        email: \"\",\n        password: \"\",\n        errors: \"\"\n    }\n\n    handleChange = (event)=>{\n        this.setState({\n          [event.target.name]: event.target.value\n        })\n    }\n\n    handleSubmit = (event)=>{\n        event.preventDefault()\n        const {username, email, password} = this.state\n        let user = {\n            username: username,\n            email: email,\n            password: password\n          }\n          \n      axios.post('http://localhost:3002/login', {user}, {withCredentials: true})\n          .then(response => {\n            if (response.data.logged_in) {\n              this.props.handleLogin(response.data)\n              this.redirect()\n            } else {\n              this.setState({\n                errors: response.data.errors\n              })\n            }\n          })\n          .catch(error => console.log('api errors:', error))\n        };\n      redirect = () => {\n          this.props.history.push('/photographers')\n        }\n    \n\n\n\n    render(){\n        return(\n            <div>\n                <h2>Login here:</h2>\n                <form onSubmit={this.handleSubmit}>\n                    <input\n                    placeholder= \"username\"\n                    type=\"text\"\n                    name=\"username\"\n                    value={this.state.username}\n                    onChange={this.handleChange}></input>\n                    <br></br>\n                    <br></br>\n                    <input\n                    placeholder=\"email\"\n                    type=\"text\"\n                    name=\"email\"\n                    value={this.state.email}\n                    onChange={this.handleChange}\n                    ></input>\n                    <br></br>\n                    <br></br>\n\n                    <input\n                    placeholder=\"password\"\n                    type=\"password\"\n                    name=\"password\"\n                    value={this.state.password}\n                    onChange={this.handleChange}\n                    ></input>\n                    <br></br>\n                    <br></br>\n\n                    <button\n                    placeholder=\"submit\"\n                    type=\"submit\"\n                    >Log in</button>\n                    \n                </form>\n            </div>\n        )\n    }\n}\n\nexport default Login","/Users/claudia/photogigs-frontend/src/actions/photographersActions.js",[],"/Users/claudia/photogigs-frontend/src/components/Review.js",[],"/Users/claudia/photogigs-frontend/src/components/Photographer.js",["109","110","111","112","113"],"import React from 'react';\nimport {Component} from 'react';\nimport Calendar from 'react-calendar';\nimport {\n     Route,\n    Link\n  } from \"react-router-dom\";\nimport LikeButton from './LikeButton'\nimport LikeButtonReactIcons from './LikeButtonReactIcons'\nimport PhotographerAvalableDates from  './PhotographerAvalableDates'\n\nclass Photographer extends Component {\n\n\nrender(){\n      return (\n        <div className='card-grid'>\n          <Route exact path = \"/photographers\">\n          <div className=\"flip-card\">\n            <div className=\"flip-card-inner\">\n              <div className = \"flip-card-front\">\n                <h2>{this.props.photographer.name}</h2>\n                <img src={this.props.photographer.image} className=\"photographer-avatar\"></img>\n              </div>\n              \n              <div className=\"flip-card-back\" >\n                <h2>{this.props.photographer.name}</h2>\n                <a target='_blank' href={this.props.photographer.website}>{this.props.photographer.website}</a>\n                <h3>YOE: {this.props.photographer.years_of_experience} </h3>\n                <h3>{this.props.photographer.city}, {this.props.photographer.state}</h3>\n                <Link to = {`/photographers/${this.props.photographer.id}`}>\n                <button id = {this.props.photographer.id} >View profile</button>\n                </Link>\n                <br></br>\n                <br></br>\n                {/* <LikeButton/> */}\n                <LikeButtonReactIcons/>\n                \n              </div>\n            </div>\n          </div>\n          \n        </Route>\n       </div>\n      )\n    }\n}\n\nexport default Photographer","/Users/claudia/photogigs-frontend/src/components/PhotographerAllInfo.js",["114","115","116","117"],"\nimport React, { Component } from 'react'\nimport PhotographerAvalableDates from './PhotographerAvalableDates'\nimport {Link} from 'react-router-dom'\n\nclass PhotographerAllInfo extends Component{\n    state ={\n        content:''\n    }\n\n    transformDate=(date)=> {\n        let newDateFormat = date.substring(0,10)  \n        return newDateFormat\n    }\n\n    \n    handleOnChange =(e) =>{\n        this.setState({\n            content: e.target.value\n        })\n    }\n\n    handleOnSubmit = (e) =>{\n        e.preventDefault()\n        this.props.createReview({content: this.state.content, photographer_id: this.props.id})\n        this.setState({content: ''})\n    }\n    \n\n    renderPhotographer(){\n        const photographer = this.props.photographers.find(photographer => photographer.id == this.props.id)\n        if (photographer){\n            return <div>\n            <h3>{photographer.name}</h3>\n            <h4>{photographer.website}</h4>\n            <h4>YOE:{photographer.years_of_experience}</h4>\n            <h4>{photographer.city}, {photographer.state}</h4>\n            </div>\n        }\n        else{\n            return <h2>photographer not found</h2>\n        }\n    }\n\n    renderReviews(){\n        //debugger\n        const photographer = this.props.photographers.find(photographer => photographer.id == this.props.id)\n        if(photographer){\n            const reviews = this.props.reviews.filter(review => review.photographer_id == photographer.id)\n            return <div>{reviews.map(review => {\n                return <div>\n                       <li key= {review.id} >{review.content} posted at: {this.transformDate(review.created_at)}</li>\n                       <br/>\n                       <br/>\n                      </div>})\n        }</div>\n        }\n    }\n\n    \n\n    render(){\n        return(\n          <div>\n            {this.renderPhotographer()}\n            <h3>See avalable dates</h3>\n            <PhotographerAvalableDates/>\n       \n            <h3>Reviews:</h3>\n            {this.renderReviews()}\n            <hr/>\n            <br/>\n            <form onSubmit={this.handleOnSubmit}>\n            <label>Add a review:</label>\n            <input type=\"text\" value={this.state.content} onChange={this.handleOnChange}/>\n            <input type=\"submit\"/>\n          </form>\n          </div>\n        )\n    }\n}\n\nexport default PhotographerAllInfo\n","/Users/claudia/photogigs-frontend/src/components/LikeButtonReactIcons.js",["118"],"import React, { Component } from 'react'\nimport {FaHeart} from 'react-icons/fa'\n\nclass LikeButtonReactIcons extends Component {\n    state={ likesCount: 0 }\n\n    handleLikeClick=(e)=>{\n    e.preventDefault();\n    let newLikeClick = this.state.likesCount+1\n    this.setState({\n        likesCount: newLikeClick\n      })\n    }\n  \n    render(){\n      return(\n          <div>\n            <label>\n                <input type=\"radio\" onClick={this.handleLikeClick}/>\n                <FaHeart\n                 color = {this.state.likesCount==0 ? \"black\" : \"orange\"}\n                 size = {18}\n                />  {this.state.likesCount}</label>\n          </div>\n      )\n    }\n}\nexport default LikeButtonReactIcons","/Users/claudia/photogigs-frontend/src/components/LikeButton.js",["119"],"import React, { Component } from 'react'\n\nclass LikeButton extends Component {\n    state={ likesCount: 0 }\n\n    displayHeart=()=>{\n        {if(this.state.likesCount === 0){\n        return <button onClick={this.handleLikeClick}>♡ {this.state.likesCount}</button>}\n          else{\n          return <button onClick={this.handleLikeClick}>♥ {this.state.likesCount}</button>}}\n      }\n\n    handleLikeClick=(e)=>{\n    e.preventDefault();\n    let newLikeClick = this.state.likesCount+1\n    this.setState({\n        likesCount: newLikeClick\n    })\n    }\n    \n\n    render(){\n      return(\n          <div>\n            {this.displayHeart()}\n          </div>\n      )\n    }\n}\n\nexport default LikeButton","/Users/claudia/photogigs-frontend/src/components/PhotographerAvalableDates.js",[],{"ruleId":"120","severity":1,"message":"121","line":15,"column":7,"nodeType":"122","messageId":"123","endLine":15,"endColumn":14},{"ruleId":"124","replacedBy":"125"},{"ruleId":"126","replacedBy":"127"},{"ruleId":"128","severity":1,"message":"129","line":3,"column":9,"nodeType":"130","messageId":"131","endLine":11,"endColumn":10},{"ruleId":"120","severity":1,"message":"132","line":3,"column":8,"nodeType":"122","messageId":"123","endLine":3,"endColumn":14},{"ruleId":"120","severity":1,"message":"133","line":4,"column":9,"nodeType":"122","messageId":"123","endLine":4,"endColumn":15},{"ruleId":"120","severity":1,"message":"134","line":4,"column":24,"nodeType":"122","messageId":"123","endLine":4,"endColumn":28},{"ruleId":"135","severity":1,"message":"136","line":26,"column":31,"nodeType":"137","messageId":"138","endLine":26,"endColumn":33},{"ruleId":"120","severity":1,"message":"139","line":3,"column":8,"nodeType":"122","messageId":"123","endLine":3,"endColumn":11},{"ruleId":"120","severity":1,"message":"140","line":3,"column":8,"nodeType":"122","messageId":"123","endLine":3,"endColumn":16},{"ruleId":"120","severity":1,"message":"141","line":8,"column":8,"nodeType":"122","messageId":"123","endLine":8,"endColumn":18},{"ruleId":"120","severity":1,"message":"142","line":10,"column":8,"nodeType":"122","messageId":"123","endLine":10,"endColumn":33},{"ruleId":"143","severity":1,"message":"144","line":23,"column":17,"nodeType":"145","endLine":23,"endColumn":90},{"ruleId":"146","severity":1,"message":"147","line":28,"column":20,"nodeType":"148","endLine":28,"endColumn":35},{"ruleId":"120","severity":1,"message":"134","line":4,"column":9,"nodeType":"122","messageId":"123","endLine":4,"endColumn":13},{"ruleId":"135","severity":1,"message":"136","line":31,"column":92,"nodeType":"137","messageId":"138","endLine":31,"endColumn":94},{"ruleId":"13