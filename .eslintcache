herAllInfo'\nimport {\n  Route\n} from \"react-router-dom\";\n\nclass Photographers extends Component {\n\n  state = {\n    searchTerm: \"\",\n    currentDisplay: this.props.photographers\n  }\n\n  handleChange =(event)=>{\n    const filteredPhotographers = this.props.photographers.filter(photographer => photographer.state.toLowerCase().includes(event.target.value.toLowerCase()))\n    console.log(filteredPhotographers)\n    this.setState({\n      searchTerm: event.target.value,\n      currentDisplay : filteredPhotographers\n    })\n  }\n\n  renderPhotographers = ()=>{\n\n    if (this.state.searchTerm == \"\"){\n    return this.props.photographers\n     .map(photographer => {\n      return <Photographer photographer={photographer} key={photographer.id} />\n      })}\n      else{\n        return this.state.currentDisplay.map(photographer => {\n          return <Photographer photographer={photographer} key={photographer.id} />\n          })\n      }\n  }\n\n  render(){\n      return (\n        <div >\n          <Route exact path = '/photographers'>\n          <div className=\"searchInput\">\n          <label>Search by state:</label>\n          <input type=\"text\" onChange={this.handleChange}/>\n          <br></br>\n          </div>\n          </Route>\n          {this.renderPhotographers()}\n          <Route exact path=\"/photographers/:id\" component={({match})=>{\n              return <PhotographerAllInfo id = {match.params.id} photographers={this.props.photographers} reviews={this.props.reviews} createReview={this.props.createReview} fetchingReviews={this.props.fetchingReviews}/>\n            }}>\n          </Route>\n          <br></br>\n          <br></br>\n        </div>\n      )\n    }\n\n\n}\n\nexport default Photographers;","/Users/claudia/photogigs-frontend/src/components/PhotographerForm.js",[],"/Users/claudia/photogigs-frontend/src/components/Signup.js",[],"/Users/claudia/photogigs-frontend/src/components/Login.js",[],"/Users/claudia/photogigs-frontend/src/actions/photographersActions.js",[],"/Users/claudia/photogigs-frontend/src/components/Review.js",[],"/Users/claudia/photogigs-frontend/src/components/Photographer.js",["118","119","120","121","122"],"import React from 'react';\nimport {Component} from 'react';\nimport Calendar from 'react-calendar';\nimport {\n     Route,\n    Link\n  } from \"react-router-dom\";\nimport LikeButton from './LikeButton'\nimport LikeButtonReactIcons from './LikeButtonReactIcons'\nimport PhotographerAvalableDates from  './PhotographerAvalableDates'\n\nclass Photographer extends Component {\n\n\nrender(){\n      return (\n        <div className='card-grid'>\n          <Route exact path = \"/photographers\">\n          <div className=\"flip-card\">\n            <div className=\"flip-card-inner\">\n              <div className = \"flip-card-front\">\n                <h2>{this.props.photographer.name}</h2>\n                <img src={this.props.photographer.image} className=\"photographer-avatar\"></img>\n              </div>\n              \n              <div className=\"flip-card-back\" >\n                <h2>{this.props.photographer.name}</h2>\n                <a target='_blank' href={this.props.photographer.website}>{this.props.photographer.website}</a>\n                <h3>YOE: {this.props.photographer.years_of_experience} </h3>\n                <h3>{this.props.photographer.city}, {this.props.photographer.state}</h3>\n                <Link to = {`/photographers/${this.props.photographer.id}`}>\n                <button id = {this.props.photographer.id} >View profile</button>\n                </Link>\n                <br></br>\n                <br></br>\n                {/* <LikeButton/> */}\n                <LikeButtonReactIcons/>\n                \n              </div>\n            </div>\n          </div>\n          \n        </Route>\n       </div>\n      )\n    }\n}\n\nexport default Photographer","/Users/claudia/photogigs-frontend/src/components/PhotographerAllInfo.js",["123","124","125","126","127","128"],"\nimport React, { Component } from 'react'\nimport PhotographerAvalableDates from './PhotographerAvalableDates'\nimport {Link} from 'react-router-dom'\n\n\n\nclass PhotographerAllInfo extends Component{\n    state ={\n        content:''\n    }\n\n    transformDate=(date)=> {\n        let newDateFormat = date.substring(0,10)  \n        return newDateFormat\n    }\n\n    \n    handleOnChange =(e) =>{\n        this.setState({\n            content: e.target.value\n        })\n    }\n\n    handleOnSubmit = (e) =>{\n        e.preventDefault()\n        this.props.createReview({content: this.state.content, photographer_id: this.props.id})\n        this.setState({content: ''})\n    }\n    \n\n    renderPhotographer(){\n        const photographer = this.props.photographers.find(photographer => photographer.id == this.props.id)\n        if (photographer){\n            return <div>\n            <h3>{photographer.name}</h3>\n            <a target='_blank' href={this.props.photographer.website}>{this.props.photographer.website}</a>\n            <h4>YOE:{photographer.years_of_experience}</h4>\n            <h4>{photographer.city}, {photographer.state}</h4>\n            </div>\n        }\n        else{\n            return <h2>photographer not found</h2>\n        }\n    }\n\n    renderReviews(){\n        //debugger\n        const photographer = this.props.photographers.find(photographer => photographer.id == this.props.id)\n        if(photographer){\n            const reviews = this.props.reviews.filter(review => review.photographer_id == photographer.id)\n            return <div>{reviews.map(review => {\n                return <div>\n                       <li key={review.id} >{review.content} posted at: {this.transformDate(review.created_at)}</li>\n                       <br/>\n                       <br/>\n                      </div>})\n        }</div>\n        }\n    }\n\n    \n\n    render(){\n        return(\n          <div>\n            {this.renderPhotographer()}\n            {/* <h3>See available dates</h3>\n            <PhotographerAvalableDates/> */}\n\n       \n            <h3>Reviews:</h3>\n            {this.renderReviews()}\n            <hr/>\n            <br/>\n            <form onSubmit={this.handleOnSubmit}>\n            <label>Add a review:</label>\n            <input type=\"text\" value={this.state.content} onChange={this.handleOnChange}/>\n            <input type=\"submit\"/>\n          </form>\n          </div>\n        )\n    }\n}\n\nexport default PhotographerAllInfo\n","/Users/claudia/photogigs-frontend/src/components/LikeButtonReactIcons.js",["129"],"import React, { Component } from 'react'\nimport {FaHeart} from 'react-icons/fa'\n\nclass LikeButtonReactIcons extends Component {\n    state={ likesCount: 0 }\n\n    handleLikeClick=(e)=>{\n    e.preventDefault();\n    let newLikeClick = this.state.likesCount+1\n    this.setState({\n        likesCount: newLikeClick\n      })\n    }\n  \n    render(){\n      return(\n          <div>\n            <label>\n                <input type=\"radio\" onClick={this.handleLikeClick}/>\n                <FaHeart\n                 color = {this.state.likesCount==0 ? \"black\" : \"orange\"}\n                 size = {18}\n                />  {this.state.likesCount}</label>\n          </div>\n      )\n    }\n}\nexport default LikeButtonReactIcons","/Users/claudia/photogigs-frontend/src/components/LikeButton.js",["130"],"import React, { Component } from 'react'\n\nclass LikeButton extends Component {\n    state={ likesCount: 0 }\n\n    displayHeart=()=>{\n        {if(this.state.likesCount === 0){\n        return <button onClick={this.handleLikeClick}>♡ {this.state.likesCount}</button>}\n          else{\n          return <button onClick={this.handleLikeClick}>♥ {this.state.likesCount}</button>}}\n      }\n\n    handleLikeClick=(e)=>{\n    e.preventDefault();\n    let newLikeClick = this.state.likesCount+1\n    this.setState({\n        likesCount: newLikeClick\n    })\n    }\n    \n\n    render(){\n      return(\n          <div>\n            {this.displayHeart()}\n          </div>\n      )\n    }\n}\n\nexport default LikeButton","/Users/claudia/photogigs-frontend/src/components/PhotographerAvalableDates.js",[],"/Users/claudia/photogigs-frontend/src/components/Logout.js",[],"/Users/claudia/photogigs-frontend/src/components/Header.js",["131"],"/Users/claudia/photogigs-frontend/src/components/Footer.js",[],{"ruleId":"132","severity":1,"message":"133","line":7,"column":40,"nodeType":"134","messageId":"135","endLine":7,"endColumn":55},{"ruleId":"136","replacedBy":"137"},{"ruleId":"138","replacedBy":"139"},{"ruleId":"132","severity":1,"message":"140","line":5,"column":8,"nodeType":"134","messageId":"135","endLine":5,"endColumn":14},{"ruleId":"132","severity":1,"message":"141","line":12,"column":9,"nodeType":"134","messageId":"135","endLine":12,"endColumn":15},{"ruleId":"142","severity":1,"message":"143","line":3,"column":9,"nodeType":"144","messageId":"145","endLine":11,"endColumn":10},{"ruleId":"136","replacedBy":"146"},{"ruleId":"138","replacedBy":"147"},{"ruleId":"132","severity":1,"message":"148","line":2,"column":8,"nodeType":"134","messageId":"135","endLine":2,"endColumn":18},{"ruleId":"149","severity":1,"message":"150","line":27,"column":31,"nodeType":"151","messageId":"152","endLine":27,"endColumn":33},{"ruleId":"132","severity":1,"message":"153","line":3,"column":8,"nodeType":"134","messageId":"135","endLine":3,"endColumn":16},{"ruleId":"132","severity":1,"message":"154","line":8,"column":8,"nodeType":"134","messageId":"135","endLine":8,"endColumn":18},{"ruleId":"132","severity":1,"message":"155","line":10,"column":8,"nodeType":"134","messageId":"135","endLine":10,"endColumn":33},{"ruleId":"156","severity":1,"message":"157","line":23,"column":17,"nodeType":"158","endLine":23,"endColumn":90},{"ruleId":"159","severity":1,"message":"160","line":28,"column":20,"nodeType":"161","endLine":28,"endColumn":35},{"ruleId":"132","sever